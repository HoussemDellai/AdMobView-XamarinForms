<?xml version="1.0" encoding="utf-8"?>
<package xmlns="http://schemas.microsoft.com/packaging/2010/07/nuspec.xsd">
  <metadata minClientVersion="2.8.1">
    <id>Xam.FormsPlugin.AdMobView</id>
    <version>1.0.1</version>
    <title>AdMobView Control Plugin for Xamarin.Forms</title>
    <authors>Houssem Dellai</authors>
    <owners>Houssem Dellai</owners>
    <licenseUrl>https://github.com/HoussemDellai/AdMobView-XamarinForms</licenseUrl>
    <projectUrl>https://github.com/HoussemDellai/AdMobView-XamarinForms</projectUrl>
    <!--Default Icon, a template can be found:  https://raw.githubusercontent.com/jamesmontemagno/Xamarin-Templates/master/Plugins-Templates/icons/plugin_icon.png-->
    <iconUrl>https://raw.githubusercontent.com/jamesmontemagno/Xamarin-Templates/master/Plugins-Templates/icons/plugin_icon_nuget.png</iconUrl>
    <requireLicenseAcceptance>false</requireLicenseAcceptance>
    <description>
      Add Google AdMob ads to your Xamarin.Forms project.
      Supported projects: Xamarin.Android and Windows Phone 8 (Silverlight).
    </description>
    <summary>Add AdMob ads to your Xamarin.Forms project.</summary>
    <tags>xamarin, pcl, xam.pcl, plugin, plugin for xamarin.forms, windows phone, winphone, wp8, android, xamarin.forms, ios</tags>
    <dependencies>
      <dependency id="Xamarin.Forms" version="1.3.1.6296" />
    </dependencies>
  </metadata>
  <files>
    <!--Core-->
    <file src="AdMobView\AdMobView.FormsPlugin.Abstractions\bin\Release\AdMobView.FormsPlugin.Abstractions.dll" target="lib\portable-net45+wp8+win8+MonoAndroid10+MonoTouch10+Xamarin.iOS10\AdMobView.FormsPlugin.Abstractions.dll" />
    <file src="AdMobView\AdMobView.FormsPlugin.Abstractions\bin\Release\AdMobView.FormsPlugin.Abstractions.xml" target="lib\portable-net45+wp8+win8+MonoAndroid10+MonoTouch10+Xamarin.iOS10\AdMobView.FormsPlugin.Abstractions.xml" />
