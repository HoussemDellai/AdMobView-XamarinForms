    <file src="AdMobView\AdMobView.FormsPlugin.WindowsPhone\bin\Release\AdMobView.FormsPlugin.WindowsPhone.xml"
          target="lib\wp8\AdMobView.FormsPlugin.WindowsPhone.xml" />

    <file src="AdMobView\AdMobView.FormsPlugin.WindowsPhone\bin\Release\AdMobView.FormsPlugin.WindowsPhone.pdb"
