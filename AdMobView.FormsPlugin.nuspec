    <file src="AdMobView\AdMobView.FormsPlugin.Abstractions\bin\Release\AdMobView.FormsPlugin.Abstractions.pdb" target="lib\MonoAndroid10\AdMobView.FormsPlugin.Abstractions.pdb" />

