    <file src="AdMobView\AdMobView.FormsPlugin.Android\bin\Release\AdMobView.FormsPlugin.Android.xml"
          target="lib\MonoAndroid10\AdMobView.FormsPlugin.Android.xml" />

    <file src="AdMobView\AdMobView.FormsPlugin.Android\bin\Release\AdMobView.FormsPlugin.Android.pdb"
